keywords = {hippocampus,hypothalamus,maternal behavior,neuroblasts,neurogenic niche,olfactory bulb,photoperiod,seasonal reproduction},
pages = {570},
pmid = {29109674},
publisher = {Frontiers Media SA},
title = {{Adult Neurogenesis in Sheep: Characterization and Contribution to Reproduction and Behavior.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29109674 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5660097},
